{
	"name": "mdsvex-playground",
	"description": "Documentation side for mdsvex",
	"version": "0.0.1",
	"scripts": {
		"dev": "sapper dev --ext '.svelte .svx'",
		"build": "sapper build --legacy --ext '.svelte .svx'",
		"export": "sapper export --legacy --ext '.svelte .svx'",
		"start": "node __sapper__/build",
		"cy:run": "cypress run",
		"cy:open": "cypress open",
		"test": "run-p --race dev cy:run",
		"generate-workers": "rollup -c rollup.workers.js"
	},
	"dependencies": {
		"compression": "^1.7.1",
		"mdsvex": "^0.5.1-beta.2",
		"polka": "^0.5.0",
		"refractor": "^2.10.0",
		"sirv": "^0.4.0",
		"yootils": "^0.0.15"
	},
	"devDependencies": {
		"@babel/core": "^7.5.5",
		"@babel/plugin-syntax-dynamic-import": "^7.0.0",
		"@babel/plugin-transform-runtime": "^7.5.5",
		"@babel/preset-env": "^7.5.5",
		"@babel/runtime": "^7.5.5",
		"codemirror": "^5.49.2",
		"npm-run-all": "^4.1.5",
		"prism-svelte": "^0.4.4",
		"rehype-autolink-headings": "^2.0.5",
		"rehype-slug": "^2.0.3",
		"remark-syntax-highlight": "^0.1.3",
		"rollup": "^1.19.4",
		"rollup-plugin-babel": "^4.3.3",
		"rollup-plugin-commonjs": "^10.0.2",
		"rollup-plugin-json": "^4.0.0",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-replace": "^2.0.0",
		"rollup-plugin-svelte": "^5.1.0",
		"rollup-plugin-terser": "^4.0.4",
		"sapper": "^0.27.8",
		"svelte": "^3.7.1",
		"svelte-json-tree": "^0.0.7",
		"typeface-catamaran": "^0.0.72",
		"typeface-roboto": "^0.0.75",
		"unist-util-visit": "^2.0.1"
	}
}
