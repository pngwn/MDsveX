{
	"name": "mdsvex",
	"version": "0.12.4-next.0",
	"description": "Markdown preprocessor for Svelte",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/main.es.d.ts",
			"require": "./dist/main.cjs",
			"default": "./dist/main.mjs"
		},
		"./package.json": "./package.json"
	},
	"main": "dist/main.cjs",
	"module": "dist/main.mjs",
	"types": "./dist/main.es.d.ts",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/pngwn/MDsveX.git"
	},
	"scripts": {
		"build": "rollup -c",
		"test": "vitest . **/*.spec.ts"
	},
	"files": [
		"dist/*",
		"globals.d.ts"
	],
	"keywords": [
		"test",
		"preprocessor",
		"mdx",
		"markdown",
		"svelte"
	],
	"author": "pngwn <hello@pngwn.io>",
	"license": "MIT",
	"devDependencies": {
		"@starptech/prettyhtml-hast-to-html": "^0.10.0",
		"@types/acorn": "^6.0.4",
		"@types/escape-html": "^1.0.4",
		"@types/estree": "^1.0.7",
		"@types/hast": "^3.0.4",
		"@types/js-yaml": "^4.0.9",
		"escape-html": "^1.0.3",
		"front-matter": "^4.0.2",
		"js-yaml": "^4.1.0",
		"node-fetch": "^3.3.2",
		"rehype-slug": "^6.0.0",
		"rehype-toc": "^3.0.2",
		"remark-autolink-headings": "^8.0.0",
		"remark-containers": "^1.2.0",
		"remark-external-links": "^10.0.0",
		"remark-frontmatter": "^5.0.0",
		"remark-parse": "^11.0.0",
		"remark-rehype": "^11.1.2",
		"remark-slug": "^8.0.0",
		"retext": "^9.0.0",
		"retext-smartypants": "^6.2.0",
		"rollup-plugin-svelte": "^7.2.2",
		"shiki": "^3.2.1",
		"svelte": "^5.25.6",
		"toml": "^3.0.0",
		"unified": "^11.0.5",
		"vfile": "^6.0.3"
	},
	"peerDependencies": {
		"svelte": "^3.56.0 || ^4.0.0 || ^5.0.0-next.120"
	},
	"dependencies": {
		"@types/unist": "^3.0.3",
		"prism-svelte": "^0.5.0",
		"prismjs": "^1.30.0",
		"vfile-message": "^4.0.2"
	}
}
