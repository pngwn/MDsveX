{
	"name": "mdsvex-playground",
	"description": "Documentation side for mdsvex",
	"version": "0.0.1",
	"scripts": {
		"dev": "sapper dev --ext '.svelte .svx'",
		"build:site": "sapper build --legacy --ext '.svelte .svx'",
		"export": "sapper export --legacy --ext '.svelte .svx'",
		"start": "node __sapper__/build",
		"cy:run": "cypress run",
		"cy:open": "cypress open",
		"test": "run-p --race dev cy:run",
		"generate-workers": "rollup -c rollup.workers.js"
	},
	"private": "true",
	"dependencies": {
		"compression": "^1.7.4",
		"mdsvex": "^0.7.1-beta.3",
		"polka": "^0.5.2",
		"refractor": "^4.2.1",
		"sirv": "^1.0.17",
		"yootils": "^0.3.1"
	},
	"devDependencies": {
		"@babel/core": "^7.15.8",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/plugin-transform-runtime": "^7.15.8",
		"@babel/preset-env": "^7.15.8",
		"@babel/runtime": "^7.15.4",
		"@rollup/plugin-babel": "^5.3.0",
		"@rollup/plugin-commonjs": "^21.0.0",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^13.0.5",
		"@rollup/plugin-replace": "^3.0.0",
		"codemirror": "^5.63.3",
		"npm-run-all": "^4.1.5",
		"prism-svelte": "^0.4.7",
		"rehype-autolink-headings": "^2.0.5",
		"rehype-slug": "^2.0.3",
		"remark-syntax-highlight": "^0.1.3",
		"rollup": "^2.58.0",
		"rollup-plugin-svelte": "^7.1.0",
		"rollup-plugin-terser": "^7.0.2",
		"sapper": "^0.29.3",
		"svelte": "3.7.1",
		"svelte-json-tree": "^0.1.0",
		"typeface-catamaran": "^1.1.13",
		"typeface-roboto": "^1.1.13",
		"unist-util-visit": "^2.0.1"
	}
}
